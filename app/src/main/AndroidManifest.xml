<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shikhar.acadCourses">

    <application
        android:name="com.shikhar.acadCourses.AcadCoursesApplication"
        android:allowBackup="true"
        android:label="Acad Courses"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.shikhar.acadCourses.listcategory.ListCategoriesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shikhar.acadCourses.listitem.ListItemsActivity"
            />
    </application>

</manifest>